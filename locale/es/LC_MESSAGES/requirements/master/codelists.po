#: codelists/dataType.csv codelists/relatesTo.csv codelists/responseSource.csv
msgid "Code"
msgstr "Código"

#: codelists/dataType.csv codelists/relatesTo.csv codelists/responseSource.csv
msgid "Title"
msgstr "Título"

#: codelists/dataType.csv codelists/relatesTo.csv codelists/responseSource.csv
msgid "Description"
msgstr "Descripción"

#. Title
#: codelists/dataType.csv:1
msgid "String"
msgstr "Cadena"

#. Description
#: codelists/dataType.csv:1
msgid "The requirement response must be of type string"
msgstr "La respuesta de requisito debe ser una cadena"

#. Title
#: codelists/dataType.csv:2
msgid "Date-time"
msgstr "Fecha y hora"

#. Description
#: codelists/dataType.csv:2
msgid "The requirement response must be of type date-time (ISO8601)"
msgstr "La respuesta de requisito debe ser del tipo fecha y hora (ISO8601)"

#. Title
#: codelists/dataType.csv:3
msgid "Number"
msgstr "Número"

#. Description
#: codelists/dataType.csv:3
msgid ""
"The requirement response must be of type number, i.e. a floating point or"
" whole number"
msgstr ""
"La respuesta de requisitos debe de ser el tipo numérica, por ejemplo: un "
"punto flotante o un número entero"

#. Title
#: codelists/dataType.csv:4
msgid "Integer"
msgstr "Entero"

#. Description
#: codelists/dataType.csv:4
msgid "The requirement response must be of type integer"
msgstr "La respuesta del requisito debe ser del tipo entero"

#. Title
#: codelists/dataType.csv:5
msgid "Boolean"
msgstr "Booleano"

#. Description
#: codelists/dataType.csv:5
msgid "The requirement response must be of type boolean"
msgstr "La respuesta de requisito debe ser de tipo booleano"

#. Title
#: codelists/relatesTo.csv:1 codelists/responseSource.csv:1
msgid "Tenderer"
msgstr "Licitante"

#. Description
#: codelists/relatesTo.csv:1
msgid "The criterion evaluates or assesses a tenderer"
msgstr "El criterio evalúa a un licitador"

#. Title
#: codelists/relatesTo.csv:2
msgid "Item"
msgstr "Artículo"

#. Description
#: codelists/relatesTo.csv:2
msgid "The criterion evaluates or assesses an item"
msgstr "El criterio evalúa un elemento"

#. Description
#: codelists/responseSource.csv:1
msgid "The response should be provided by the tenderer"
msgstr "La respuesta deberá ser proporcionada por el licitante"

#. Title
#: codelists/responseSource.csv:2
msgid "Buyer"
msgstr "Comprador"

#. Description
#: codelists/responseSource.csv:2
msgid "The response should be provided by the buyer"
msgstr "La respuesta debe ser proporcionada por el comprador"

#. Title
#: codelists/responseSource.csv:3
msgid "Procuring entity"
msgstr "Entidad contratante"

#. Description
#: codelists/responseSource.csv:3
msgid "The response should be provided by the procuring entity"
msgstr "La respuesta debe ser proporcionada por la entidad adjudicadora"
